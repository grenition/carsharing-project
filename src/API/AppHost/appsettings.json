{
  "App": {
    "Name": "F-API"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Postgres": "Host=localhost;Port=5432;Database=main;Username=123;Password=123"
  },
  "Jwt": {
    "Audience": "https://f-universe.ru",
    "Issuer": "https://f-universe.ru",
    "ExpirationMinutes": 30
  },
  "TwoFactor": {
    "CodeLength": 6,
    "ExpirationMinutes": 5
  },
  "Smtp": {
    "ServerAddress": "smtp.f-universe.ru",
    "ServerPort": 587
  },
  "Cors": {
    "DefaultPolicy": {
      "Origins": [ "https://f-universe.ru" ]
    }
  },
  "Environment": "Production"
}
